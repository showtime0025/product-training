{
  "CardProps": {
    "title": {
      "type": "string",
      "required": false,
      "description": "The title text displayed at the top of the card"
    },
    "subtitle": {
      "type": "string",
      "required": false,
      "description": "The subtitle text displayed below the title"
    },
    "children": {
      "type": "React.ReactNode",
      "required": true,
      "description": "The main content of the card"
    },
    "variant": {
      "type": "'default' | 'outlined' | 'elevated'",
      "required": false,
      "default": "default",
      "description": "Visual variant of the card"
    },
    "padding": {
      "type": "'none' | 'sm' | 'md' | 'lg'",
      "required": false,
      "default": "md",
      "description": "Internal padding of the card"
    },
    "className": {
      "type": "string",
      "required": false,
      "description": "Additional CSS class names"
    },
    "onClick": {
      "type": "() => void",
      "required": false,
      "description": "Callback function when card is clicked"
    }
  }
}
